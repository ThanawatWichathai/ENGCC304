#include <stdio.h>

int main() {
    float height = 0 ;
	  float base = 0 ;
	  float area = 0 ;

    printf( "H = " ) ;
    scanf( "%f", &height ) ;

    printf( "W = " ) ;
    scanf( "%f", &base ) ;

    area = ( height * base ) / 2 ;

    printf( "Area = %.1f\n", area ) ;

    return 0 ;
}//end function

