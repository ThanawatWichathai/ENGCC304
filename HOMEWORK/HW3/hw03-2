#include <stdio.h>
#include <math.h>

int isPrime( int num ) {
    if( num < 2 ) {
        return 0 ;
    }//end if
    for( int i = 2 ; i <= (int)sqrt(num) ; i++ ) {
        if( num % i == 0 ) {
            return 0 ;
        }//end if
    }//end for
    return 1 ;
}//end function

int main() {
    int n ;

    printf( "Enter N : ") ;
    scanf( "%d", &n ) ;

    int arr[n] ;

    for( int i = 0 ; i < n ; i++ ) {
        printf( "Enter value[%d] : ", i ) ;
        scanf( "%d", &arr[i] ) ;
    }//end for

    printf( "Index:  " ) ;
    for( int i = 0 ; i < n ; i++ ) {
        printf( "%3d", i ) ;
    }//end for
    
    printf( "\nArray:  " ) ;
    for( int i = 0 ; i < n ; i++ ) {
        if( isPrime(arr[i]) ) {
            printf( "%3d", arr[i] ) ;
        } else {
            printf( "%3s", "#" ) ;
        }//end if
    }//end for

    return 0 ;
}//end function
